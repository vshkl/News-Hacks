CREATE TABLE itemEntity (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    pubDate TEXT NOT NULL,
    link TEXT NOT NULL,
    commentsLink TEXT NOT NULL
);

insert:
INSERT OR REPLACE
INTO itemEntity(title, pubDate, link, commentsLink)
VALUES (?, ?, ?, ?);

selectAll:
SELECT *
FROM itemEntity;

deleteAll:
DELETE FROM itemEntity;
